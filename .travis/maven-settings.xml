<settings>
	<servers>
		<server>
			<id>ossrh</id>
			<username>${env.OSSRH_USERNAME}</username>
			<password>${env.OSSRH_PASSWORD}</password>
		</server>
	</servers>
	<profiles>
		<profile>
			<id>ossrh</id>
			<properties>
				<gpg.executable>${env.GPG_EXECUTABLE}</gpg.executable>
				<gpg.passphrase>${env.GPG_PASSPHRASE}</gpg.passphrase>
				<gpg.useagent>true</gpg.useagent>
			</properties>
		</profile>
	</profiles>
</settings>